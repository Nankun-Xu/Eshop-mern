  "bugs": {
    "url": "https://github.com/Nankun-Xu/Eshop-mern/issues"
  },
  "homepage": "https://github.com/Nankun-Xu/Eshop-mern#readme",
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "mongoose": "^8.0.3"
  }
}
